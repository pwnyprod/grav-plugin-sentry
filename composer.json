{
    "name": "pwnyprod/grav-plugin-sentry",
    "type": "project",
    "description": "Sentry Bugtracker integration for Grav",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Hoppe",
            "email": "pwnyprod@gmail.com",
